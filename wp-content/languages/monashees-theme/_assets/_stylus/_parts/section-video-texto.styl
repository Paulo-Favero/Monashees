.section-video-texto
	padding 800px 0 0 0
	margin-top -650px
	margin-bottom -850px
	position relative
	background url('_assets/_imgs/_elements/bottom-video.png?v=1')  center top no-repeat
	.texto-bg-branco
		height 500px
		padding-top 100px
		background #FFF
	.texto-video
		padding-top 100px
		height 1200px
		position relative
		.texto-1
			width 660px
			position absolute
			left 50%
			margin-top -600px
			margin-left -330px
			z-index 10
			h2
				font-size  55px
				font-family "AntennaThin"
				color #000
				line-height 65px
				transition(color .2s ease-in-out)
		.texto-2
			padding-top 240px
			position relative
			z-index 100
			p
				color #FFF
				font-size 21px
				strong
					font-family "AntennaThin"
	



	
			
.texto-corret
	width 660px
	position absolute
	left 50%
	margin-top 0 !important
	margin-left -330px

.texto-fixed
	position fixed !important
	z-index 1000
	top 50% !important
	left 50% 
	margin-left - 330px
	margin-top - 75px !important

.color-white
	h2
		color #FFF !important
		


.texto-video:before
	content ""
	width 100%
	height 200%
	position absolute
	top 0
	left 0
	z-index 1
	background rgba(0,0,0,0.6)
	

